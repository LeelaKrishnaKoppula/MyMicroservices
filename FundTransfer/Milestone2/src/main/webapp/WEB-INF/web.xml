<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>NewCustomerServlet</servlet-name>
  	<display-name>NewCustomerServlet</display-name>
  	<description></description>
  	<servlet-class>com.wipro.bank.controller.NewCustomerServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FundTransferServlet</servlet-name>
  	<display-name>FundTransferServlet</display-name>
  	<description></description>
  	<servlet-class>com.wipro.bank.controller.FundTransferServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetAccountBalanceServlet</servlet-name>
  	<display-name>GetAccountBalanceServlet</display-name>
  	<description></description>
  	<servlet-class>com.wipro.bank.controller.GetAccountBalanceServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>NewCustomerServlet</servlet-name>
  	<url-pattern>/NewCustomerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FundTransferServlet</servlet-name>
  	<url-pattern>/FundTransferServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetAccountBalanceServlet</servlet-name>
  	<url-pattern>/GetAccountBalanceServlet</url-pattern>
  </servlet-mapping>
<welcome-file-list>
<welcome-file>Home.jsp</welcome-file>
</welcome-file-list>
</web-app>
